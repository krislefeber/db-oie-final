{
  "name": "hessen-web",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "setup": "npm install -g @angular/cli nodemon ts-node typings concurrently && typings install && npm install",
    "start": "concurrently \"nodemon -e ts,js,json --watch server --exec ts-node server/bin/www.ts\" \"ng serve --proxy-config proxy.conf.json &\"",
    "build": "npm run build:app && npm run build:server",
    "build:app": "ng build --prod --aot --output-path=dist/client",
    "build:server": "tsc -p ./server",
    "lint": "ng lint",
    "build:run": "node dist/server/bin/www.js",
    "build:server:live": "sudo nodemon"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.1.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@covalent/core": "^1.0.0-beta.3-2",
    "concurrently": "^3.4.0",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express-myconnection": "^1.0.4",
    "express-prettify": "0.0.7",
    "express-serve-static-core": "^0.1.1",
    "express-session": "^1.15.2",
    "hammerjs": "^2.0.8",
    "mysql": "^2.13.0",
    "nodemon": "^1.11.0",
    "rxjs": "^5.1.0",
    "typescript": "^2.3.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "protractor": "~5.1.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
